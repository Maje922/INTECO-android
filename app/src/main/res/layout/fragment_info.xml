<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".info">


    <ImageButton
        android:id="@+id/B_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="83dp"
        android:layout_marginTop="132dp"
        android:background="@android:color/transparent"
        android:src="@mipmap/ic_email" />

    <ImageButton
        android:id="@+id/B_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/B_tel"
        android:layout_alignTop="@+id/B_pdf"
        android:background="@android:color/transparent"
        android:src="@mipmap/ic_gps" />

    <ImageButton
        android:id="@+id/B_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/B_email"
        android:layout_marginEnd="74dp"
        android:background="@android:color/transparent"
        android:src="@mipmap/ic_tel" />

    <ImageButton
        android:id="@+id/B_pdf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/B_email"
        android:layout_marginBottom="142dp"
        android:background="@android:color/transparent"
        android:src="@mipmap/ic_pdf" />
</RelativeLayout>